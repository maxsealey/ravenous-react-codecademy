          return (<li key = {sortByOptionValue} className = {this.getSortByClass(sortByOptionValue)} onClick = {this.handleSortByChange.bind(this,sortByOptionValue)}>\n                {sortByOption}</li>) // returns list item \n        }) \n    }// returns array of list items, renders each item \n\n    handleTermChange(e) { // event gets passed in as argument\n        this.setState({term: e.target.value})\n    } // event handler for change of search term\n\n    handleLocationChange(e) {\n        this.setState({location: e.target.value})\n    } // event handler for change in location\n\n    handleSearch(e) {\n        this.props.searchYelp(this.state.term, this.state.location, this.state.sortBy)\n        e.preventDefault()\n    } // event handler for Search yelp method\n\n    render() {\n        return (\n        <div className=\"SearchBar\">\n            <div className=\"SearchBar-sort-options\">\n                <ul>\n                {this.renderSortByOptions()}\n                </ul>\n            </div>\n            <div className=\"SearchBar-fields\">\n                <input placeholder=\"Search Businesses\" onChange={this.handleTermChange} />\n                <input placeholder=\"Where?\" onChange={this.handleLocationChange} />\n            </div>\n            <div className=\"SearchBar-submit\">\n                <a onClick={this.handleSearch}>Let's Go</a>\n            </div>\n        </div>\n        );\n    }\n}\n\nexport default SearchBar;","/Users/maxsealey/Development/personal/ravenous-react-codecademy/src/components/Business/Business.js",["40","41"],"/Users/maxsealey/Development/personal/ravenous-react-codecademy/src/util/Yelp.js",[],{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","severity":1,"message":"47","line":9,"column":5,"nodeType":"48","messageId":"49","endLine":9,"endColumn":9},{"ruleId":"46","severity":1,"message":"47","line":12,"column":5,"nodeType":"48","messageId":"49","endLine":12,"endColumn":9},{"ruleId":"46"